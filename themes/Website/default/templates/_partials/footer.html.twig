<div class="footer__btm">
    <div class="container">
        <ul class="footer__links">
            {% for menu in menus %}
            <li>
            	{% if menu.menuType == 'none' %}
                <span>{{ menu.name }}</span>
                {% else %}
                <a href="{{ menuPath(menu) }}" {% if menu.blank %}target="_blank"{% endif %}>
                    {{ menu.name }}
                </a>
                {% endif %}
            </li>
            {% endfor %}
        </ul>
    </div>
</div>
